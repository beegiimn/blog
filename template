    function loadOlderPosts() {
        var link = document.getElementById("olderPostsLink");
        var currentUrl = link.href;
        var updatedUrl = currentUrl.replace(/updated-max=[^&]+/, "updated-max=" + getNextDate());
        link.href = updatedUrl;
    }
    function getNextDate() {
        // code to get the date of the last post in the current set of posts
        // and return the date of the post before it
    }
    
    <a href='http://beegiimn.blogspot.com/search?updated-max=2020-01-01T00:00:00-07:00&max-results=10' id="olderPostsLink" 
